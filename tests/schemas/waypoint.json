{
   "$schema":"http://json-schema.org/draft-04/schema#",
   "title":"Waypoint Schema",
   "type":"object",
   "properties":{
      "x":{
         "description":"The x coordinates of a Point",
         "type":"integer"
      },
      "y":{
         "description":"The y coordinates of a Point",
         "type":"integer"
      },
      "z":{
         "description":"The z coordinates of a Point",
         "type":"integer"
      }
   },
   "required":[
      "x",
      "y",
      "z"
   ]
}
